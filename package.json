{
  "name": "screening",
  "version": "1.0.2",
  "description": "Tools for numbering screening plate",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root Screening",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheminfo-js/screening.git"
  },
  "keywords": [
    "screening",
    "plate"
  ],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo-js/screening/issues"
  },
  "homepage": "https://github.com/cheminfo-js/screning",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.14",
    "cheminfo-build": "^1.3.1",
    "eslint": "^9.39.1",
    "eslint-config-cheminfo": "^17.0.0",
    "prettier": "^3.7.3",
    "rollup": "^4.53.3",
    "vitest": "^4.0.14"
  }
}
